<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.pams.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginCredentials</servlet-name>
  	<display-name>LoginCredentials</display-name>
  	<description></description>
  	<servlet-class>com.revature.pams.model.LoginCredentials</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PamsMain</servlet-name>
  	<display-name>PamsMain</display-name>
  	<description></description>
  	<servlet-class>com.revature.pams.dao.PamsMain</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddingDetails</servlet-name>
  	<display-name>AddingDetails</display-name>
  	<description></description>
  	<servlet-class>com.revature.Servlet.AddingDetails</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateServlet</servlet-name>
  	<display-name>UpdateServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.Servlet.UpdateServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditDetails</servlet-name>
  	<display-name>EditDetails</display-name>
  	<description></description>
  	<servlet-class>com.revature.Servlet.EditDetails</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteServlet</servlet-name>
  	<display-name>DeleteServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.Servlet.DeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginCredentials</servlet-name>
  	<url-pattern>/LoginCredentials</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PamsMain</servlet-name>
  	<url-pattern>/PamsMain</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddingDetails</servlet-name>
  	<url-pattern>/AddingDetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServlet</servlet-name>
  	<url-pattern>/UpdateServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditDetails</servlet-name>
  	<url-pattern>/EditDetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServlet</servlet-name>
  	<url-pattern>/DeleteServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
